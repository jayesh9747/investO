{
  "expo": {
    "name": "InvestO",
    "slug": "investo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "investo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Investment tracking and analysis app",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jayesh.investo"
    },
    "extra": {
      "apiUrl": "https://www.alphavantage.co",
      "apiKey": "797CTOAYKKSHXZSF"
    },
    "android": {
      "package": "com.jayesh.investo",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}